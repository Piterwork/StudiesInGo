package main

import "fmt"

func main() {
	x := 10
	y := 20

	a := (x == y)
	b := (x <= y)
	c := (x < y)
	d := (x > y)
	e := (x >= y)
	f := (x != y)
	fmt.Printf("%v\n%v\n%v\n%v\n%v\n%v\n", a, b, c, d, e, f)
}

// Escreva expressões utilizando os operadores, e atribua seus valores a variáveis.
// Demonstre estes valores.
// Solução: https://play.golang.org/p/BMYEch6_s8
